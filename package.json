{
  "name": "framework-demo",
  "version": "1.0.1",
  "description": "egg.js framework demo",
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-mysql": "^3.0.0",
    "egg-view-handlebars": "^2.0.1"
  },
  "devDependencies": {
    "autod": "^3",
    "autod-egg": "^1",
    "egg-bin": "^4",
    "egg-ci": "^1",
    "egg-mock": "^4",
    "eslint": "^7",
    "eslint-config-egg": "^8.0.0"
  },
  "egg": {
    "declaration": true,
    "tsHelper": {
      "watchDirs": {
        "enum": {
          "enabled": true,
          "directory": "app/enum",
          "declareTo": "Application.enum"
        },
        "util": {
          "enabled": true,
          "directory": "app/util",
          "declareTo": "Application.util"
        }
      }
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint . --resolve-plugins-relative-to .",
    "ci": "npm run lint && egg-bin pkgfiles --check && npm run cov",
    "autod": "autod",
    "pkgfiles": "egg-bin pkgfiles",
    "typing": "npx ets"
  },
  "ci": {
    "version": "12",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Samaritan89/egg-framework-demo.git"
  },
  "keywords": [
    "egg",
    "egg-framework"
  ],
  "author": "Sunluyong",
  "files": [
    "index.js",
    "lib",
    "app",
    "config",
    "app.js"
  ],
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "license": "MIT"
}
